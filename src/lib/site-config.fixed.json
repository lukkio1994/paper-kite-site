{
  "site": {
    "branding": {
      "name": "t('config.header.logo.text')",
      "logoText": "t('config.header.logo.text')",
      "logoHref": "/"
    }
  },
  "header": {
    "logo": {
      "text": "t('config.header.logo.text')",
      "href": "/",
      "className": ""
    },
    "navigation": [
      {
        "label": "t('config.header.navigation.features')",
        "href": "/features",
        "subItems": [
          {
            "label": "t('config.header.navigation.featuresAnalytics')",
            "href": "/features/analytics",
            "description": "t('config.header.navigation.featuresAnalyticsDescription')"
          },
          {
            "label": "t('config.header.navigation.featuresCollaboration')",
            "href": "/features/collaboration",
            "description": "t('config.header.navigation.featuresCollaborationDescription')"
          }
        ]
      },
      {
        "label": "t('config.header.navigation.pricing')",
        "href": "/pricing"
      },
      {
        "label": "t('config.header.navigation.about')",
        "href": "/about"
      }
    ],
    "actions": [
      {
        "label": "t('config.header.actions.signIn')",
        "href": "/auth/signin",
        "variant": "ghost",
        "size": "sm"
      },
      {
        "label": "t('config.header.actions.getStarted')",
        "href": "/auth/signup",
        "variant": "primary",
        "size": "sm"
      }
    ],
    "appearance": {
      "variant": "elevated",
      "background": "default",
      "sticky": true,
      "showBorder": true,
      "height": "default",
      "mobileMenuBreakpoint": "md",
      "navigationAlignment": "right",
      "className": ""
    },
    "mobile": {
      "showLogo": true,
      "showActions": true,
      "menuPosition": "right",
      "overlayBackground": "blur"
    },
    "accessibility": {
      "skipToContentHref": "#main-content",
      "logoAriaLabel": "t('config.header.accessibility.logoAriaLabel')",
      "mobileMenuAriaLabel": "t('config.header.accessibility.mobileMenuAriaLabel')",
      "navigationAriaLabel": "t('config.header.accessibility.navigationAriaLabel')",
      "mainLandmarkLabel": "t('config.header.accessibility.mainLandmarkLabel')",
      "searchFormLabel": "t('config.header.accessibility.searchFormLabel')",
      "languageSwitcherLabel": "t('config.header.accessibility.languageSwitcherLabel')"
    }
  },
  "footer": {
    "contact": {
      "address": "t('config.footer.contact.address')",
      "phone": "t('config.footer.contact.phone')",
      "email": "t('config.footer.contact.email')"
    },
    "socialLinks": [
      {
        "platform": "t('config.footer.social.twitter')",
        "href": "https://twitter.com/yourcompany"
      },
      {
        "platform": "t('config.footer.social.linkedin')",
        "href": "https://linkedin.com/company/yourcompany"
      },
      {
        "platform": "t('config.footer.social.github')",
        "href": "https://github.com/yourcompany"
      }
    ],
    "legalLinks": [
      {
        "label": "t('config.footer.legal.privacy')",
        "href": "/privacy"
      },
      {
        "label": "t('config.footer.legal.terms')",
        "href": "/terms"
      }
    ],
    "productLinks": [
      {
        "label": "t('config.footer.product.features')",
        "href": "/features"
      },
      {
        "label": "t('config.footer.product.pricing')",
        "href": "/pricing"
      }
    ],
    "appearance": {
      "variant": "detailed",
      "background": "dark",
      "showBackToTop": true,
      "showDividers": true,
      "className": "w-screen max-w-none !mx-0 !px-0"
    },
    "solutionLinks": [],
    "resourceLinks": [],
    "supportLinks": [],
    "companyLinks": [],
    "accessibility": {
      "footerAriaLabel": "t('config.footer.accessibility.footerAriaLabel')",
      "socialLinksAriaLabel": "t('config.footer.accessibility.socialLinksAriaLabel')",
      "legalLinksAriaLabel": "t('config.footer.accessibility.legalLinksAriaLabel')",
      "productLinksAriaLabel": "t('config.footer.accessibility.productLinksAriaLabel')",
      "contactInfoAriaLabel": "t('config.footer.accessibility.contactInfoAriaLabel')",
      "backToTopAriaLabel": "t('config.footer.accessibility.backToTopAriaLabel')",
      "copyrightAriaLabel": "t('config.footer.accessibility.copyrightAriaLabel')"
    },
    "copyright": "t('config.footer.copyright')"
  }
}
